{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "@*": [
        "src/*"
      ],
      "@@*": [
        "assets/*"
      ],
      "@@@*": [
        "public/*"
      ]
    }
  }
}